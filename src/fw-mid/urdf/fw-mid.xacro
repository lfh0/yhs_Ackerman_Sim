            </geometry>
            <origin xyz="0.0 0.0 0.0" rpy="1.57 0.0 0.0" />
        </collision>
        <inertial>
            <origin xyz="0 0 0" /> 
            <mass value="4" />
            <inertia 
            ixx="0.01603" ixy="0" ixz="0"
            iyy="0.01603" iyz="0"
            izz="0.0288" 
            />
        </inertial>
    </link>

    <joint name="left_front_wheel_joint" type="continuous">
        <parent link="left_steering_hinge" />
        <child link="left_front_wheel" />
        <origin xyz="0 0.0 0" />
        <axis xyz="0 1 0" />
